@{
    ViewData["Title"] = @ViewBag.ServerName;
}
<h2>Server Name: @ViewBag.ServerName</h2>

<h5>Labs ID:</h5>
@for (int i = 0; i < ((object[])ViewBag.Labs2).Length; i++)
{
    <a href="@Url.Action("LabInfo","CML",new {id=ViewBag.ServerID, servername=ViewBag.ServerName, id_lab=ViewBag.Labs2[i]})"><p>@ViewBag.Labs2[i]</p> </a>
}